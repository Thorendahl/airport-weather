{
    "name": "airport-weather",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test --watch",
        "e2e": "ng e2e",
        "copy-docs": "node copy-to-docs.js",
        "build:docs": "npm run build && npm run copy-docs"
    },
    "prettier": {
        "printWidth": 100,
        "singleQuote": true,
        "overrides": [
            {
                "files": "*.html",
                "options": {
                    "parser": "angular"
                }
            }
        ]
    },
    "private": true,
    "dependencies": {
        "@angular/common": "^20.1.0",
        "@angular/compiler": "^20.1.0",
        "@angular/core": "^20.1.0",
        "@angular/forms": "^20.1.0",
        "@angular/platform-browser": "^20.1.0",
        "@angular/router": "^20.1.0",
        "@fortawesome/fontawesome-free": "^7.0.0",
        "date-fns": "^4.1.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0"
    },
    "devDependencies": {
        "@analogjs/platform": "^1.19.4",
        "@analogjs/vite-plugin-angular": "^1.19.4",
        "@analogjs/vitest-angular": "^1.19.4",
        "@angular/build": "^20.1.6",
        "@angular/cli": "^20.1.6",
        "@angular/compiler-cli": "^20.1.0",
        "@nx/vite": "^21.0.0",
        "@playwright/test": "1.54.2",
        "@types/jasmine": "~5.1.0",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "jasmine-core": "~5.8.0",
        "jsdom": "^22.0.0",
        "playwright-ng-schematics": "^2.1.0",
        "typescript": "~5.8.2",
        "vite": "^6.0.0",
        "vite-tsconfig-paths": "^4.2.0",
        "vitest": "^3.2.4"
    }
}
